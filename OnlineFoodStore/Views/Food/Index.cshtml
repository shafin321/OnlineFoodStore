
@model IEnumerable<OnlineFoodStore.Models.Food>


@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<div>
    <h2>All drinks</h2>
    @foreach (var food in Model)
    {
        <div>
            <br />
            <h4>Drink name: @food.Name</h4>
            <h3>Category name:  @food.Category.Name</h3>
            <p>Price: @food.Price.ToString("c")</p>
        </div>
    }
</div>
